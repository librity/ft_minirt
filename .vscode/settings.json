{
  "editor.tabSize": 4,
  "editor.insertSpaces": false,
  "editor.renderWhitespace": "all",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.detectIndentation": true,

  "[c]": {
    "editor.defaultFormatter": "keyhr.42-c-format"
  },
  "files.associations": {
    "*.gohtml": "html",
    "*.s": "plaintext",
    "minirt.h": "c",
    "stdbool.h": "c",
    "fcntl.h": "c",
    "ft_libbmp.h": "c",
    "stdlib.h": "c",
    "defines.h": "c",
    "warnings.h": "c",
    "structs.h": "c",
    "*.tcc": "c",
    "cmath": "c",
    "random": "c",
    "numbers": "c",
    "atomic": "c",
    "compare": "c",
    "numeric": "c",
    "functional": "c"
  }
}
